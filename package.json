{
  "name": "@tmdjr/ngx-editorjs-demo",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-editor-demo",
    "gh-deploy": "git push origin :gh-pages && git subtree push --prefix dist/ngx-editor-demo origin gh-pages",
    "gh-deploy-init": "git push origin && git subtree push --prefix dist/ngx-editor-demo origin gh-pages",
    "build": "ng build ngx-editor-demo --base-href /ngx-editorjs/ --configuration development",
    "build1": "ng build ngx-editorjs --configuration production",
    "build2": "ng build ngx-editorjs-client --configuration production",
    "build3": "ng build ngx-editorjs-paragraph-block --configuration production",
    "build4": "ng build ngx-editorjs-blockquotes-block --configuration production",
    "build5": "ng build ngx-editorjs-image-block --configuration production",
    "build6": "ng build ngx-editorjs-code-block --configuration production",
    "build7": "ng build ngx-editorjs-quiz-block --configuration production",
    "build8": "ng build ngx-editorjs-mermaid-block --configuration production",
    "watch1": "ng build ngx-editorjs --watch --configuration development",
    "watch2": "ng build ngx-editorjs-client --watch --configuration development",
    "watch3": "ng build ngx-editorjs-paragraph-block --watch --configuration development",
    "watch4": "ng build ngx-editorjs-blockquotes-block --watch --configuration development",
    "watch5": "ng build ngx-editorjs-image-block --watch --configuration development",
    "watch6": "ng build ngx-editorjs-code-block --watch --configuration development",
    "watch7": "ng build ngx-editorjs-quiz-block --watch --configuration development",
    "watch8": "ng build ngx-editorjs-mermaid-block --watch --configuration development",
    "watch": "npm run watch1 & npm run watch3 & npm run watch4 & npm run watch5 & npm run watch6",
    "test": "ng test",
    "linkEditor": "cd dist/ngx-editorjs && npm link && cd ../../",
    "linkEditorClient": "cd dist/ngx-editorjs-client && npm link && cd ../../",
    "linkEditorParagraph": "cd dist/ngx-editorjs-paragraph-block && npm link && cd ../../",
    "linkEditorBlockquotes": "cd dist/ngx-editorjs-blockquotes-block && npm link && cd ../../",
    "linkEditorImage": "cd dist/ngx-editorjs-image-block && npm link && cd ../../",
    "linkEditorCode": "cd dist/ngx-editorjs-code-block && npm link && cd ../../",
    "linkEditorQuiz": "cd dist/ngx-editorjs-quiz-block && npm link && cd ../../",
    "linkEditorMermaid": "cd dist/ngx-editorjs-mermaid-block && npm link && cd ../../",
    "linkAll": "npm run linkEditor && npm run linkEditorClient && npm run linkEditorParagraph && npm run linkEditorBlockquotes && npm run linkEditorImage && npm run linkEditorCode && npm run linkEditorQuiz && npm run linkEditorMermaid",
    "link": "npm link @tmdjr/ngx-editorjs @tmdjr/ngx-editorjs-client @tmdjr/ngx-editorjs-paragraph-block @tmdjr/ngx-editorjs-blockquotes-block @tmdjr/ngx-editorjs-image-block @tmdjr/ngx-editorjs-code-block @tmdjr/ngx-editorjs-quiz-block @tmdjr/ngx-editorjs-mermaid-block",
    "uninstallAll": "npm uninstall -g @tmdjr/ngx-editorjs @tmdjr/ngx-editorjs-client @tmdjr/ngx-editorjs-blockquotes-block @tmdjr/ngx-editorjs-client @tmdjr/ngx-editorjs-code-block @tmdjr/ngx-editorjs-image-block @tmdjr/ngx-editorjs-paragraph-block @tmdjr/ngx-editorjs-quiz-block @tmdjr/ngx-editorjs-mermaid-block",
    "publishEditor": "cd dist/ngx-editorjs && npm publish && cd ../../",
    "publishEditorClient": "cd dist/ngx-editorjs-client && npm publish && cd ../../",
    "publishEditorParagraph": "cd dist/ngx-editorjs-paragraph-block && npm publish && cd ../../",
    "publishEditorBlockquotes": "cd dist/ngx-editorjs-blockquotes-block && npm publish && cd ../../",
    "publishEditorImage": "cd dist/ngx-editorjs-image-block && npm publish && cd ../../",
    "publishEditorCode": "cd dist/ngx-editorjs-code-block && npm publish && cd ../../",
    "publishEditorQuiz": "cd dist/ngx-editorjs-quiz-block && npm publish && cd ../../",
    "publishEditorMermaid": "cd dist/ngx-editorjs-mermaid-block && npm publish && cd ../../",
    "publishAll": "npm run publishEditor && npm run publishEditorClient && npm run publishEditorParagraph && npm run publishEditorBlockquotes && npm run publishEditorImage && npm run publishEditorCode && npm run publishEditorQuiz && npm run publishEditorMermaid",
    "buildAndLinkAll": "npm run build1 && npm run build2 && npm run build3 && npm run build4 && npm run build5 && npm run build6 && npm run build7 && npm run build8 && npm run linkAll"
  },
  "private": false,
  "dependencies": {
  },
  "devDependencies": {
  }
}