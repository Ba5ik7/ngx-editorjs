{
  "name": "@tmdjr/ngx-editorjs-demo",
  "version": "0.0.32",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-editor-demo --open",
    "gh-deploy": "git push origin :gh-pages && git subtree push --prefix dist/ngx-editor-demo origin gh-pages",
    "gh-deploy-init": "git push origin && git subtree push --prefix dist/ngx-editor-demo origin gh-pages",
    "build": "ng build ngx-editor-demo --base-href /ngx-editorjs/ --configuration development",
    "watch1": "ng build ngx-editorjs --watch --configuration development",
    "watch2": "ng build ngx-editorjs-client --watch --configuration development",
    "watch3": "ng build ngx-editorjs-paragraph-block --watch --configuration development",
    "watch4": "ng build ngx-editorjs-blockquotes-block --watch --configuration development",
    "watch5": "ng build ngx-editorjs-image-block --watch --configuration development",
    "watch6": "ng build ngx-editorjs-code-block --watch --configuration development",
    "watch": "npm run watch1 & npm run watch3 & npm run watch4 & npm run watch5 & npm run watch6",
    "test": "ng test",
    "linkEditor": "cd dist/ngx-editorjs && npm link && cd ../../",
    "linkEditorClient": "cd dist/ngx-editorjs-client && npm link && cd ../../",
    "linkEditorParagraph": "cd dist/ngx-editorjs-paragraph-block && npm link && cd ../../",
    "linkEditorBlockquotes": "cd dist/ngx-editorjs-blockquotes-block && npm link && cd ../../",
    "linkEditorImage": "cd dist/ngx-editorjs-image-block && npm link && cd ../../",
    "linkEditorCode": "cd dist/ngx-editorjs-code-block && npm link && cd ../../",
    "linkAll": "npm run linkEditor && npm run linkEditorClient && npm run linkEditorParagraph && npm run linkEditorBlockquotes && npm run linkEditorImage && npm run linkEditorCode",
    "link": "npm link @tmdjr/ngx-editorjs @tmdjr/ngx-editorjs-client @tmdjr/ngx-editorjs-paragraph-block @tmdjr/ngx-editorjs-blockquotes-block @tmdjr/ngx-editorjs-image-block @tmdjr/ngx-editorjs-code-block",
    "publishEditor": "cd dist/ngx-editorjs && npm publish && cd ../../",
    "publishEditorClient": "cd dist/ngx-editorjs-client && npm publish && cd ../../",
    "publishEditorParagraph": "cd dist/ngx-editorjs-paragraph-block && npm publish && cd ../../",
    "publishEditorBlockquotes": "cd dist/ngx-editorjs-blockquotes-block && npm publish && cd ../../",
    "publishEditorImage": "cd dist/ngx-editorjs-image-block && npm publish && cd ../../",
    "publishEditorCode": "cd dist/ngx-editorjs-code-block && npm publish && cd ../../",
    "publishAll": "npm run publishEditor && npm run publishEditorClient && npm run publishEditorParagraph && npm run publishEditorBlockquotes && npm run publishEditorImage && npm run publishEditorCode"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~15.2.0",
    "@angular/cdk": "^15.2.0",
    "@angular/common": "~15.2.0",
    "@angular/compiler": "~15.2.0",
    "@angular/core": "~15.2.0",
    "@angular/forms": "~15.2.0",
    "@angular/material": "^15.2.0",
    "@angular/platform-browser": "~15.2.0",
    "@angular/platform-browser-dynamic": "~15.2.0",
    "@angular/router": "~15.2.0",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.2.0",
    "@angular/cli": "~15.2.0",
    "@angular/compiler-cli": "~15.2.0",
    "@ctrl/ngx-codemirror": "^5.1.1",
    "@types/codemirror": "5.60.5",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.7.15",
    "codemirror": "5.64.0",
    "jasmine-core": "~4.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.2.2",
    "schematics-scss-migrate": "^1.3.15",
    "typescript": "4.9.5"
  }
}
